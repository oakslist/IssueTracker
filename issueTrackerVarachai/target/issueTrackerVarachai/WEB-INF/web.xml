<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<resource-ref>
		<description>DBConnection</description>
		<res-ref-name>jdbc/H2db</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>
	<display-name>issueTrackerVarachai</display-name>
	<servlet>
		<servlet-name>LoginController</servlet-name>
		<servlet-class>org.training.controllers.LoginController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SessionController</servlet-name>
		<servlet-class>org.training.controllers.SessionController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LogoutController</servlet-name>
		<servlet-class>org.training.controllers.LogoutController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SubmitIssueController</servlet-name>
		<servlet-class>org.training.controllers.SubmitIssueController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>BeforeSubmitIssueController</servlet-name>
		<servlet-class>org.training.controllers.BeforeSubmitIssueController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>BeforeEditIssueController</servlet-name>
		<servlet-class>org.training.controllers.BeforeEditIssueController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EditIssueController</servlet-name>
		<servlet-class>org.training.controllers.EditIssueController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddUserController</servlet-name>
		<servlet-class>org.training.controllers.AddUserController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EditUserController</servlet-name>
		<servlet-class>org.training.controllers.EditUserController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EditPasswordController</servlet-name>
		<servlet-class>org.training.controllers.EditPasswordController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>BeforeSearchUserController</servlet-name>
		<servlet-class>org.training.controllers.BeforeSearchUserController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EditDifferentUserController</servlet-name>
		<servlet-class>org.training.controllers.EditDifferentUserController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EditDifferentUserPasswordController</servlet-name>
		<servlet-class>org.training.controllers.EditDifferentUserPasswordController</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>SessionController</servlet-name>
		<url-pattern>/SessionController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginController</servlet-name>
		<url-pattern>/LoginController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LogoutController</servlet-name>
		<url-pattern>/LogoutController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SubmitIssueController</servlet-name>
		<url-pattern>/SubmitIssueController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>BeforeSubmitIssueController</servlet-name>
		<url-pattern>/BeforeSubmitIssueController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>BeforeEditIssueController</servlet-name>
		<url-pattern>/BeforeEditIssueController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EditIssueController</servlet-name>
		<url-pattern>/EditIssueController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddUserController</servlet-name>
		<url-pattern>/AddUserController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EditUserController</servlet-name>
		<url-pattern>/EditUserController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EditPasswordController</servlet-name>
		<url-pattern>/EditPasswordController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>BeforeSearchUserController</servlet-name>
		<url-pattern>/BeforeSearchUserController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EditDifferentUserController</servlet-name>
		<url-pattern>/EditDifferentUserController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
	<servlet-name>EditDifferentUserPasswordController</servlet-name>
		<url-pattern>/EditDifferentUserPasswordController</url-pattern>
	</servlet-mapping>
</web-app>