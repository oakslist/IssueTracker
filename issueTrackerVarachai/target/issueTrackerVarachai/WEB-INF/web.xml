<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>issueTrackerVarachai</display-name>
	<servlet>
		<servlet-name>IndexPageController</servlet-name>
		<servlet-class>by.epam.controllers.IndexPageController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SignInResultPageController</servlet-name>
		<servlet-class>by.epam.controllers.SignInResultPageController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CheckUserController</servlet-name>
		<servlet-class>by.epam.controllers.CheckUserController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>IndexPageController</servlet-name>
		<url-pattern>/index.html</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SignInResultPageController</servlet-name>
		<url-pattern>/welcome.html</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CheckUserController</servlet-name>
		<url-pattern>/CheckUserController</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>